    "n = 5\n",
    "k = 3\n",
    "\n",
    "\n",
    "def fib(n, k): \n",
    "    new, old = 1, 1\n",
    "    for i in range(n-1): \n",
    "        new, old = old, old + (new*k)\n",
    "    return new"
